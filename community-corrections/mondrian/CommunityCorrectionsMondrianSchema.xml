    <Table name="ArrestView">
      <ColumnDefs>
        <CalculatedColumnDef name='SixMonthRebookingStatus' type='Integer'>
          <ExpressionView>
            <SQL dialect="mysql">
              CASE WHEN <Column name="DaysSinceLastBooking"/> IS NULL THEN NULL
                WHEN <Column name="DaysSinceLastBooking"/> &lt;= 180 THEN 1
                ELSE 0 END
            </SQL>
          </ExpressionView>
        </CalculatedColumnDef>
        <CalculatedColumnDef name='OneYearRebookingStatus' type='Integer'>
          <ExpressionView>
            <SQL dialect="mysql">
              CASE WHEN <Column name="DaysSinceLastBooking"/> IS NULL THEN NULL
                WHEN <Column name="DaysSinceLastBooking"/> &lt;= 365 THEN 1
                ELSE 0 END
            </SQL>
          </ExpressionView>
        </CalculatedColumnDef>
        <CalculatedColumnDef name='TwoYearRebookingStatus' type='Integer'>
          <ExpressionView>
            <SQL dialect="mysql">
              CASE WHEN <Column name="DaysSinceLastBooking"/> IS NULL THEN NULL
                WHEN <Column name="DaysSinceLastBooking"/> &lt;= 730 THEN 1
                ELSE 0 END
            </SQL>
          </ExpressionView>
        </CalculatedColumnDef>
      </ColumnDefs>
    </Table>

    <Table name="ChargeView"/>
    <Table name="ProbationSupervisionView"/>
		<Table name="ProbationSupervisionStatusView"/>
    <Table name="ProbationTermArrestView">
      <ColumnDefs>
        <CalculatedColumnDef name='SixMonthRebookingStatus' type='Integer'>
          <ExpressionView>
            <SQL dialect="mysql">
              CASE WHEN <Column name="DaysSinceLastBooking"/> IS NULL THEN NULL
                WHEN <Column name="DaysSinceLastBooking"/> &lt;= 180 THEN 1
                ELSE 0 END
            </SQL>
          </ExpressionView>
        </CalculatedColumnDef>
        <CalculatedColumnDef name='OneYearRebookingStatus' type='Integer'>
          <ExpressionView>
            <SQL dialect="mysql">
              CASE WHEN <Column name="DaysSinceLastBooking"/> IS NULL THEN NULL
                WHEN <Column name="DaysSinceLastBooking"/> &lt;= 365 THEN 1
                ELSE 0 END
            </SQL>
          </ExpressionView>
        </CalculatedColumnDef>
        <CalculatedColumnDef name='TwoYearRebookingStatus' type='Integer'>
          <ExpressionView>
            <SQL dialect="mysql">
              CASE WHEN <Column name="DaysSinceLastBooking"/> IS NULL THEN NULL
                WHEN <Column name="DaysSinceLastBooking"/> &lt;= 730 THEN 1
                ELSE 0 END
            </SQL>
          </ExpressionView>
        </CalculatedColumnDef>
      </ColumnDefs>
    </Table>

    <Query alias="PersonAgeType">
      <ExpressionView>
        <SQL dialect="generic">
SELECT PersonAgeTypeID, AgeInYears, AgeRange, AgeRangeSort from PersonAgeType inner join PersonAgeRangeType on PersonAgeType.PersonAgeRangeTypeID=PersonAgeRangeType.PersonAgeRangeTypeID
        </SQL>
      </ExpressionView>
    </Query>

    <InlineTable alias="RebookingStatusType">
      <ColumnDefs>
        <ColumnDef name="RebookingStatusTypeID" type="Integer"/>
        <ColumnDef name="RebookingStatusTypeDescription" type="String"/>
      </ColumnDefs>
      <Rows>
        <Row>
          <Value column="RebookingStatusTypeID">1</Value>
          <Value column="RebookingStatusDescription">Yes</Value>
        </Row>
        <Row>
          <Value column="RebookingStatusTypeID">0</Value>
          <Value column="RebookingStatusTypeDescription">No</Value>
        </Row>
      </Rows>
    </InlineTable>

