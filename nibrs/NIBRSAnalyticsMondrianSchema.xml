	<Cube name="VictimOffender" caption="Victim-Offender Relationship" visible="true" enabled="true">
		<View alias="VictimOffender">
			<SQL dialect="mysql"><![CDATA[select VictimSegment.*,
				OffenderSegment.AgeOfOffender, OffenderSegment.SexOfPersonTypeID as SexOfPersonTypeID_Offender,
				OffenderSegment.RaceOfPersonTypeID as RaceOfPersonTypeID_Offender,
				OffenderSegment.EthnicityOfPersonTypeID as EthnicityOfPersonTypeID_Offender,   
				VictimOffenderAssociationID, VictimOffenderRelationshipTypeID, AdministrativeSegment.AgencyID, AdministrativeSegment.ClearedExceptionallyTypeID
			from VictimSegment, OffenderSegment, AdministrativeSegment, VictimOffenderAssociation
			where VictimSegment.AdministrativeSegmentID=AdministrativeSegment.AdministrativeSegmentID and
				OffenderSegment.AdministrativeSegmentID=AdministrativeSegment.AdministrativeSegmentID and
				VictimOffenderAssociation.OffenderSegmentID=OffenderSegment.OffenderSegmentID and
				VictimOffenderAssociation.VictimSegmentID=VictimSegment.VictimSegmentID]]></SQL>
		</View>
		<DimensionUsage source="County" name="County" visible="true" foreignKey="AgencyID" highCardinality="false"/>
		<DimensionUsage source="Agency" name="Agency" visible="true" foreignKey="AgencyID" highCardinality="false"/>
		<DimensionUsage source="ClearedExceptionally" name="ClearedExceptionally" caption="Exceptional Clearance" visible="true" foreignKey="ClearedExceptionallyTypeID" highCardinality="false"/>
		<DimensionUsage source="SexOfPerson" name="SexOfPerson_Victim" caption="Victim Sex" visible="true" foreignKey="SexOfPersonTypeID" highCardinality="false"/>
		<DimensionUsage source="RaceOfPerson" name="RaceOfPerson_Victim" caption="Victim Race" visible="true" foreignKey="RaceOfPersonTypeID" highCardinality="false"/>
		<DimensionUsage source="EthnicityOfPerson" name="EthnicityOfPerson_Victim" caption="Victim Ethnicity" visible="true" foreignKey="EthnicityOfPersonTypeID" highCardinality="false"/>
		<DimensionUsage source="ResidentStatusOfPerson" name="ResidentStatusOfPerson_Victim" caption="Victim Residence Status" visible="true" foreignKey="ResidentStatusOfPersonTypeID" highCardinality="false"/>
		<DimensionUsage source="TypeOfVictim" name="TypeOfVictim" caption="Victim Type" visible="true" foreignKey="TypeOfVictimTypeID" highCardinality="false"/>
		<Dimension name="AgeOfPerson_Victim" caption="Victim Age">
			<Hierarchy hasAll="true">
				<Level name="Victim Age" column="AgeOfVictim" uniqueMembers="true"/>
			</Hierarchy>
		</Dimension>
		<DimensionUsage source="SexOfPerson" name="SexOfPerson_Offender" caption="Offender Sex" visible="true" foreignKey="SexOfPersonTypeID_Offender" highCardinality="false"/>
		<DimensionUsage source="RaceOfPerson" name="RaceOfPerson_Offender" caption="Offender Race" visible="true" foreignKey="RaceOfPersonTypeID_Offender" highCardinality="false"/>
		<DimensionUsage source="EthnicityOfPerson" name="EthnicityOfPerson_Offender" caption="Offender Ethnicity" visible="true" foreignKey="EthnicityOfPersonTypeID_Offender" highCardinality="false"/>
		<Dimension name="AgeOfPerson_Offender" caption="Offender Age">
			<Hierarchy hasAll="true">
				<Level name="Offender Age" column="AgeOfOffender" uniqueMembers="true"/>
			</Hierarchy>
		</Dimension>
		<DimensionUsage source="VictimOffenderRelationship" name="VictimOffenderRelationship" caption="Relationship" visible="true" foreignKey="VictimOffenderRelationshipTypeID" highCardinality="false"/>
		<Measure name="VictimOffenderCount" column="VictimOffenderAssociationID" datatype="Integer" aggregator="count" caption="Victim-Offender Count" visible="true"/>
	</Cube>
